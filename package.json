{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "prepare": "husky",
    "knex:make": "cross-env NODE_ENV=development cross-env  KNEX_VALUE=true   knex migrate:make -x ts --knexfile src/db/configdb.ts",
    "knex:migrate": "cross-env NODE_ENV=development cross-env KNEX_VALUE=true   knex migrate:latest --knexfile src/db/configdb.ts",
    "knex:rollback": "cross-env NODE_ENV=development cross-env KNEX_VALUE=true   knex migrate:rollback --knexfile src/db/configdb.ts"
  },
  "dependencies": {
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "pg": "^8.16.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "@types/webpack-node-externals": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "nodemon-webpack-plugin": "^4.8.2",
    "prettier": "^3.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-node-externals": "^3.0.0"
  }
}
